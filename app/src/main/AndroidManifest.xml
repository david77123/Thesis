            android:label="@string/title_activity_punnett_square"
            android:parentActivityName="com.deepanetta.thesis.Main">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value="com.deepanetta.thesis.Main" />
        </activity>
    </application>

</manifest>

